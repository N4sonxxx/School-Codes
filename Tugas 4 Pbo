package com.company;

public class Main {

    public static void main(String[] args) {
        String str1, str2;
        boolean b1=true, b2=false;
        int a=3, b=7;
        str1="Hasilnya adalah = ";
//Operator String
        System.out.println(str1);
//Operator Logika
        System.out.println(b1||b2);
//Operator Aritmatika
        System.out.println(a++*2);
        System.out.println(++a*2);
//Operasi Bit
        System.out.println(a>>2);
        System.out.println(a<<2);
//Operator Assignment
        a+=b;
        System.out.println(a);
        }
        }

